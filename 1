#include <iostream>
#include <string>
using namespace std;
bool proverka(string str)
{
	setlocale(LC_ALL, "Rus");
	int len = str.length();    //количество символов
	for (int i = 0; i < len / 2; i++)
	{
		if (str[i] != str[len - i - 1])
		{
			return false;
		}
	}
	return true; 
}
int main()
{
	string str;
	cin >> str;
	if(proverka(str)==true) {cout<< "слово является палиндромом";}
	else { cout << "слово не является палиндромом"; }
}
